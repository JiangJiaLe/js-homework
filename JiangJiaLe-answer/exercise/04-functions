// JavaScript source code
var num = new Array();
var i;
for (i = 0; ; i++) {
    num[i] = prompt("Please some figures.Until the figure be less than -1000", " ");
    if (num[i] < -1000) break;
};
function min(a) {
    var min = 9999999;
    var j;
    for (j = 0; j < a.length - 1; j++) {
        if (a[j] < min) {
            min = a[j];
        };
    };
    document.write("Minimum number=" + min);
    console.log(min);
};
min(num);
